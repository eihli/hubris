// Ignition Server IPC interface

Interface(
    name: "Ignition",
    ops: {
        "state": (
            doc: "Return the state of the given controller",
            args: {
                "id": "u8",
            },
            reply: Result(
                ok: (
                    type: "Target",
                    recv: FromPrimitive("u64"),
                ),
                err: CLike("IgnitionError"),
            ),
        ),
    }
)
